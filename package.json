{
  "name": "private-snippets",
  "displayName": "Private Snippets",
  "description": "Load custom snippets from .vscode/snippets/*.json files",
  "version": "0.1.3",
  "publisher": "brianuceda",
  "license": "MIT",
	"icon": "images/private-snippets-logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/brianuceda/private-snippets"
	},
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage",
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "private-snippets.reload",
        "title": "Reload Private Snippets"
      },
      {
        "command": "private-snippets.addSnippet",
        "title": "Add New Snippet"
      }
    ]
  }
}
